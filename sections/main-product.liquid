<script>
  let btnGetBootSize =  document.querySelector('.btn-get-boot-size');
  let specialMessage = document.querySelector('.special-message');
  let recommendedBootSize = document.getElementById('recommended_boot_size');
  let findSizeInput = document.querySelector('.find-size-input');
  let findSizeOutput = document.querySelector('.find-size-output');

  if(btnGetBootSize) {
    btnGetBootSize.addEventListener('click', function(e) {
      let footLength = document.getElementById('foot_length').value;

      if(footLength != 'null') {
        if(boot_sizes[footLength] < min_size || boot_sizes[footLength] > max_size){
          specialMessage.classList.remove('hidden');
        } else {
          specialMessage.classList.add('hidden');
          recommendedBootSize.innerHTML = boot_sizes[footLength]
          findSizeInput.classList.add('hidden')
          findSizeOutput.classList.remove('hidden')
        }
      }
    });
  }

  if(findSizeOutput) {
    findSizeOutput.addEventListener('click', function(e) {
      findSizeInput.classList.remove('hidden')
      findSizeOutput.classList.add('hidden')
    });
  }
</script>

<script type="module">
  let scrollContainer = document.querySelector('#page-product .MagicScroll');
  let thumbsWrapper = document.querySelector('.safetynet-slider-thumbs');
  let thumbs = document.querySelectorAll('.safetynet-slider-thumbs .safetynetSlideImage');
  
  window.MagicScrollOptions = {
    onMoveEnd: function (index) {
      let currShown = index.items[0];
      let currActive = document.querySelector('.safetynetSlideActive');
      
      currActive.classList.remove('safetynetSlideActive');
      thumbsWrapper.dataset.pos = currShown;
      thumbs[currShown].classList.add('safetynetSlideActive');
    }
  };

  if (scrollContainer && thumbs.length> 0) {
    let prev;
    let next;
    let handle = setInterval(function(e) {
      prev = document.querySelector('#page-product .MagicScroll .mcs-button-arrow-prev');
      next = document.querySelector('#page-product .MagicScroll .mcs-button-arrow-next');
      
      if (prev && next) {
        clearInterval(handle);
        attachListeners();
      }
    }, 50);

    for (let i = 0 ; i < thumbs.length ; i++) {
      thumbs[i].addEventListener('click', function(e) {
      MagicScroll.jump(i);
      document.querySelector('.safetynet-slider-thumbs .safetynetSlideImage.safetynetSlideActive').classList.remove('safetynetSlideActive');
      thumbs[i].classList.add('safetynetSlideActive');
    })
  }

  

  // Listen to MagicScroll arrows for thumbnail highlighting
  let clickFlag = true;
  
  function attachListeners() {
    prev.addEventListener('click', function(e) {
    
      if (clickFlag) {
        let currActive = document.querySelector('.safetynetSlideActive');
      
        if (parseInt(thumbsWrapper.dataset.pos) == 0) {
          thumbsWrapper.dataset.pos = thumbs.length - 1;
          thumbs[thumbs.length - 1].classList.add('safetynetSlideActive');
        } else {
          thumbsWrapper.dataset.pos = parseInt(thumbsWrapper.dataset.pos) - 1;
          currActive.previousElementSibling.classList.add('safetynetSlideActive');
        }
        
        currActive.classList.remove('safetynetSlideActive');
        clickFlag = false;
        
        setTimeout(function(e) {
          clickFlag = true;
        }, 600);
      }
    })

    next.addEventListener('click', function(e) {
      if (clickFlag) {
      
        let currActive = document.querySelector('.safetynetSlideActive');
        
        if (parseInt(thumbsWrapper.dataset.pos) == thumbs.length - 1) {
          thumbsWrapper.dataset.pos = 0;
          thumbs[0].classList.add('safetynetSlideActive');
        } else {
          thumbsWrapper.dataset.pos = parseInt(thumbsWrapper.dataset.pos) + 1;
          currActive.nextElementSibling.classList.add('safetynetSlideActive');
        }
        
        currActive.classList.remove('safetynetSlideActive');
        clickFlag = false;
        
        setTimeout(function(e) {
          clickFlag = true;
        }, 600);
      }
    })
  }
}
</script>

